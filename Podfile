# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'Hoteling' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Hoteling
  
  #Side Menu
  pod 'SideMenuController'
  #Date picker
  pod 'JBDatePicker'
  #Google Authentication
  pod 'GoogleSignIn'
  #Activity Indicator
  pod 'PKHUD', '~> 5.0'
  #Network Management
  pod 'Alamofire', '~> 4.7.0'
  pod 'AlamofireImage', '~> 3.3'
  #Date Manager
  pod 'SwiftDate'
  #Alert Controller Utils
  pod 'UIAlertController+Blocks'
  #Image view with zoom
  pod 'ZoomImageView'
  #Bade for remaining reservations
  pod 'BadgeSwift', '~> 5.0'
  #Crashlytics
  pod 'Fabric'
  pod 'Crashlytics'
  #Internet connection
  pod 'ReachabilitySwift'
  #Banner for connection warnings
  pod 'NotificationBannerSwift'
  #Accordion Table View
  pod 'FZAccordionTableView'
end
